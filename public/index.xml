      <title>Hi, I&#39;m Trashwbin</title>
      <link>http://localhost:1313/home/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>http://localhost:1313/home/</guid>
      <description>&lt;div class=&#34;hero-content&#34; style=&#34;text-align: center; padding: 2rem;&#34;&gt;&#xD;&#xA;  &#xD;&#xA;&#xD;&#xA;  &#xD;&#xA;  &lt;h1 class=&#34;welcome-title&#34;&gt;&#xD;&#xA;    &lt;span class=&#34;welcome-line&#34;&gt;Ê¨¢ËøéÊù•Âà∞&lt;/span&gt;&#xD;&#xA;    &lt;span class=&#34;welcome-line&#34;&gt;ÊàëÁöÑÁ©∫Èó¥&lt;/span&gt;&#xD;&#xA;    &lt;span class=&#34;welcome-line&#34;&gt;üöÄ&lt;/span&gt;&#xD;&#xA;  &lt;/h1&gt;&#xD;&#xA;  &#xD;&#xA;&#xD;&#xA;  &lt;div style=&#34;font-size: 1.2rem; color: #666;&#34;&gt;&#xD;&#xA;    &#xD;&#xA;&#xD;&#xA;    &#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&#xD;&#xA;  &lt;div class=&#34;tech-stack&#34; style=&#34;margin-top: 2rem; display: flex; justify-content: center; gap: 1rem; flex-wrap: wrap;&#34;&gt;&#xD;&#xA;    &#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;style&gt;&#xD;&#xA;  .welcome-title {&#xD;&#xA;    font-size: 2.5rem;&#xD;&#xA;    margin-bottom: 1rem;&#xD;&#xA;    display: flex;&#xD;&#xA;    flex-direction: column;&#xD;&#xA;    align-items: center;&#xD;&#xA;    gap: 0.5rem;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .welcome-line:nth-child(1),&#xD;&#xA;  .welcome-line:nth-child(2) {&#xD;&#xA;    background: linear-gradient(120deg, #f39c12, #e74c3c);&#xD;&#xA;    -webkit-background-clip: text;&#xD;&#xA;    background-clip: text;&#xD;&#xA;    color: transparent;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @media (min-width: 769px) {&#xD;&#xA;    .welcome-title {&#xD;&#xA;      flex-direction: row;&#xD;&#xA;      gap: 0.5rem;&#xD;&#xA;      justify-content: center;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    .welcome-line:last-child {&#xD;&#xA;      margin-left: 0.5rem;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .title-name-switch {&#xD;&#xA;    position: relative;&#xD;&#xA;    display: inline-block;&#xD;&#xA;    min-width: 120px;&#xD;&#xA;    color: #f39c12;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .title-name-switch .title-name-switch-original {&#xD;&#xA;    animation: fadeOriginal 6s infinite;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .title-name-switch::after {&#xD;&#xA;    content: &#39;Abin  &#39;;&#xD;&#xA;    position: absolute;&#xD;&#xA;    left: 0;&#xD;&#xA;    top: 0;&#xD;&#xA;    width: 0;&#xD;&#xA;    opacity: 0;&#xD;&#xA;    color: #e74c3c;&#xD;&#xA;    overflow: hidden;&#xD;&#xA;    white-space: nowrap;&#xD;&#xA;    animation: typing 6s steps(20) infinite;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @keyframes typing {&#xD;&#xA;&#xD;&#xA;    0%,&#xD;&#xA;    45% {&#xD;&#xA;      width: 0;&#xD;&#xA;      opacity: 0;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    50%,&#xD;&#xA;    95% {&#xD;&#xA;      width: 100%;&#xD;&#xA;      opacity: 1;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    100% {&#xD;&#xA;      width: 0;&#xD;&#xA;      opacity: 0;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @keyframes fadeOriginal {&#xD;&#xA;&#xD;&#xA;    0%,&#xD;&#xA;    45% {&#xD;&#xA;      opacity: 1;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    50%,&#xD;&#xA;    95% {&#xD;&#xA;      opacity: 0;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    100% {&#xD;&#xA;      opacity: 1;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @media (max-width: 768px) {&#xD;&#xA;    .title-name-switch {&#xD;&#xA;      min-width: 90px;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;&lt;/style&gt;&#xD;&#xA;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;  document.addEventListener(&#39;DOMContentLoaded&#39;, function () {&#xD;&#xA;    const nameSwitch = document.querySelector(&#39;.title-name-switch&#39;);&#xD;&#xA;    if (nameSwitch) {&#xD;&#xA;      nameSwitch.addEventListener(&#39;mouseenter&#39;, function () {&#xD;&#xA;        this.style.animationPlayState = &#39;paused&#39;;&#xD;&#xA;      });&#xD;&#xA;&#xD;&#xA;      nameSwitch.addEventListener(&#39;mouseleave&#39;, function () {&#xD;&#xA;        this.style.animationPlayState = &#39;running&#39;;&#xD;&#xA;      });&#xD;&#xA;    }&#xD;&#xA;  });&#xD;&#xA;&lt;/script&gt;</description>
